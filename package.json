{
  "name": "next-starter-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@hookform/resolvers": "3.1.1",
    "@reduxjs/toolkit": "1.8.6",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "axios": "1.1.3",
    "classnames": "2.3.2",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "next": "13.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.0",
    "react-input-mask": "2.0.4",
    "react-redux": "8.0.4",
    "react-tabs": "6.0.1",
    "redux-saga": "1.2.1",
    "sass": "1.55.0",
    "swiper": "10.0.3",
    "typed-redux-saga": "1.3.1",
    "typescript": "5.1.3",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@jambit/eslint-plugin-typed-redux-saga": "0.4.0",
    "@svgr/webpack": "6.5.0",
    "@types/react-input-mask": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "babel-plugin-macros": "3.1.0",
    "lint-staged": "13.2.0",
    "prettier": "2.8.4",
    "prettier-plugin-sorted": "2.0.0",
    "simple-git-hooks": "2.8.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css}": "prettier --write"
  },
  "importSort": {
    ".js, jsx, .ts, .tsx": {
      "options": {
        "bottomAliases": [
          "*.module.scss"
        ]
      }
    }
  }
}
